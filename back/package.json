{
  "name": "teste-tecnico-hagens-back",
  "version": "1.0.0",
  "description": "API de um app para cadastro, edição e login de usuários",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "NODE_ENV=dev tsx watch src/server.ts",
    "migration:run": "prisma migrate deploy",
    "migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:compose": "docker-compose -f docker-compose.development.yml --env-file .env.dev",
    "dev:docker": "npm run dev:compose -- up -d",
    "dev:docker:down": "npm run dev:compose -- down",
    "dev:docker:rebuild": "npm run dev:compose -- up --build -d",
    "dev:load-envs": "dotenv -e .env.dev",
    "dev:prisma:studio": "npm run dev:load-envs prisma studio",
    "prod:docker": "docker-compose up -d",
    "prod:docker:down": "docker-compose down"
  },
  "keywords": [],
  "author": "Lucas Gabriel de Lima Silva",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cryptr": "^6.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.7.1",
    "jsonwebtoken": "^9.0.0"
  }
}
